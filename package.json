{
  "name": "dechat_es1a",
  "version": "0.0.0",
  "description": "Decentralized chat",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test ang": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test-with-coverage": "nyc --reporter text-lcov node_modules/.bin/_mocha --require @babel/register -R spec --timeout 100 && cat ./coverage/lcov.info | codacy-coverage && rm -rf ./coverage",
    "travisRepo": "./test/travisRepo.sh",
    "travis-deploy-once": "travis-deploy-once",
    "puml": "./node_modules/.bin/puml",
    "doc": "./node_modules/.bin/asciidoctorjs  src/docs/index.adoc  --attributes data-uri --destination-dir docs/doc",
    "test": "jest && protractor ./protractor.conf.js",
    "ci": "ng serve -o && jest && protractor ./protractor.conf.js"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "rootDir": "."
  },
  "author": "Arquisoft/ES1A",
  "license": "MIT",
  "browser": {
    "crypto": false,
    "fetch": false,
    "window": false
  },
  "private": true,
